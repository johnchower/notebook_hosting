    "    ,  y_var = seq(5, 25, 5)\n",
    ")\n",
    "\n",
    "test_data\n",
    "\n",
    "ggplot(test_data, aes(x = x_var, y = y_var)) + \n",
    "    geom_bar(stat = 'identity')"
